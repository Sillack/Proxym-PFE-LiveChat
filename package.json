{
  "name": "chat-conference-plugin",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng build @proxym/ngx-chat && ng serve",
    "clean": "rimraf out-tsc && rimraf dist && rimraf coverage && rimraf documentation",
    "compile": "tsc -p projects/proxym/ngx-chat/tsconfig.lib.json --noEmit --noImplicitAny",
    "build-lib": "npm run compile && npm run clean && ng build @proxym/ngx-chat --prod && node fix-build-result.js",
    "build": "ng build",
    "test": "ng test @proxym/ngx-chat --watch",
    "test:once": "ng test @proxym/ngx-chat --code-coverage --watch=false",
    "lint": "ng lint @proxym/ngx-chat",
    "e2e": "ng e2e",
    "bundle-report": "webpack-bundle-analyzer dist/chat-conference-plugin/stats-es5.json",
    "documentation": "compodoc --tsconfig projects/proxym/ngx-chat/tsconfig.lib.json",
    "documentation:serve": "compodoc --tsconfig projects/proxym/ngx-chat/tsconfig.lib.json --serve --watch",
    "changelog": "conventional-changelog --infile CHANGELOG.md --same-file --preset angular --pkg projects/proxym/ngx-chat/package.json --release-count 0"
  },
  "dependencies": {
    "@angular/animations": "~9.0.4",
    "@angular/cdk": "^9.1.1",
    "@angular/common": "~9.0.4",
    "@angular/compiler": "~9.0.4",
    "@angular/core": "~9.0.4",
    "@angular/forms": "~9.0.4",
    "@angular/localize": "~9.0.4",
    "@angular/material": "^9.1.1",
    "@angular/platform-browser": "~9.0.4",
    "@angular/platform-browser-dynamic": "~9.0.4",
    "@angular/router": "~9.0.4",
    "@ctrl/ngx-emoji-mart": "^1.0.1",
    "@ng-bootstrap/ng-bootstrap": "^6.1.0",
    "@ngmodule/material-carousel": "^0.6.0",
    "@syncfusion/ej2-angular-grids": "^18.1.48",
    "@syncfusion/ej2-angular-splitbuttons": "^18.1.43",
    "@types/ltx": "^2.6.3",
    "@xmpp/client": "^0.9.2",
    "angular-bootstrap-md": "^9.3.1",
    "angular-feather": "^6.0.10",
    "bootstrap": "^4.4.0",
    "cordova-android": "^8.1.0",
    "cordova-browser": "^6.0.0",
    "cordova-plugin-camera": "^4.1.0",
    "cordova-plugin-device": "^2.0.3",
    "giphy-api": "^2.0.1",
    "mat-video": "^2.8.1",
    "mdbootstrap": "^4.19.1",
    "rxjs": "^6.5.4",
    "skeleton-css": "^2.0.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.4",
    "@angular-devkit/build-ng-packagr": "~0.900.4",
    "@angular/cli": "~9.0.4",
    "@angular/compiler-cli": "~9.0.4",
    "@angular/language-service": "~9.0.4",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.0.0",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "^3.5.10",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "concat-files": "^0.1.1",
    "conventional-changelog-cli": "^2.0.34",
    "coveralls": "^3.0.2",
    "glob": "^7.1.2",
    "husky": "^1.1.3",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^5.0.5",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "ng-packagr": "^9.0.0",
    "protractor": "^5.4.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.20.1",
    "typescript": "3.6.4",
    "webpack-bundle-analyzer": "^3.6.1",
    "cordova-plugin-whitelist": "^1.3.4"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-device": {},
      "cordova-plugin-camera": {
        "ANDROID_SUPPORT_V4_VERSION": "27.+"
      }
    },
    "platforms": [
      "android",
      "browser"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
